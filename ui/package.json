{
  "type": "module",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "rsbuild build",
    "dev": "rsbuild build --watch --env-mode=development",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "prettier": "prettier --write src/",
    "test:unit": "vitest --passWithNoTests",
    "type-check": "vue-tsc --build"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@halo-dev/api-client": "^2.22.0",
    "@halo-dev/components": "^2.22.0",
    "@halo-dev/richtext-editor": "^2.22.0",
    "@halo-dev/ui-shared": "^2.22.0",
    "@iconify-json/simple-icons": "^1.2.63",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@halo-dev/ui-plugin-bundler-kit": "^2.22.0",
    "@iconify/json": "^2.2.421",
    "@rsbuild/core": "^1.6.15",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rspack/core": "^1.7.2",
    "@tsconfig/node20": "^20.1.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.27",
    "@vitest/eslint-plugin": "^1.6.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-vue": "~10.0.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.12",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "typescript": "~5.8.3",
    "unplugin-icons": "^22.5.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.25.0"
}
